<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kuku">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_header_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_header_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity
            android:name=".BadHabits"
            android:label="@string/title_activity_bad_habits"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".BestPractice"
            android:label="@string/title_activity_best_practice"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".CommonDiseases"
            android:label="@string/title_activity_common_diseases"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".PoultryManagement"
            android:label="Poultry management"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="kuku"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BreedsActivity"
            android:label="Breeds"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".HousingAndEquipment"
            android:label="Housing and equipment"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".Brooding"
            android:label="Brooding"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    </application>

</manifest>